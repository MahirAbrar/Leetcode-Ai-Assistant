{
  "manifest_version": 3,
  "name": "LeetCode AI Assistant",
  "version": "1.0",
  "description": "AI assistant to help with solving LeetCode problems",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon16.png",
    "48": "icons/icon16.png",
    "128": "icons/icon16.png"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "https://leetcode.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon16.png",
      "48": "icons/icon16.png",
      "128": "icons/icon16.png"
    },
    "default_title": "LeetCode AI Assistant"
  },
  "web_accessible_resources": [
    {
      "resources": ["icons/*"],
      "matches": ["https://leetcode.com/*"]
    }
  ]
} 